---
// Component Imports
import { ViewTransitions } from "astro:transitions";
import '../styles/base.css';
import Head from '../components/Head.astro'
import Gtag from '../components/gtag.astro'
import Navbar from '../components/Navbar.astro'
import Menu from '../components/Menu.astro'
import Footer from '../components/Footer.astro'
import SideBarNav from '../components/SideBarNav.astro';
import ResourceCard from '../components/ResourceCard.astro';
import ResourceGridAll from "../components/ResourceGridAll.astro";

---

<html lang="en">
<head>
    <ViewTransitions />
    <Head Title='Resources'/>
    <script defer src="playground/app.js"></script>

    <style>
    @media (max-width: 976px) {  

    .sidebar {
        position: fixed;
        left: 16.923vw;
        top: 92vh;
        width: 258px;
        transition: width 0.3s ease, opacity 0.3s ease;
    }
     
    .sidebar a,
    .sidebar .break {
        opacity: 0;
        display: none;
        transition: opacity 0.3s ease;
    }

   .fixed {
        top: 50vw;
    }

    .sidebar.closed {
        width: 100%;
        transition: width 0.3s ease, opacity 0.3s ease;
    }

    .sidebar.closed a,
    .sidebar.closed .break {
            opacity: 1;
            display: block;
            transition: opacity 0.3s ease;
        }
}
    
    @media (min-width: 976px) {

        .sidebar {
            width: 258px;
            transition: width 0.3s ease, opacity 0.3s ease;
        }
        
        .sidebar.closed {
            width: 65px;
        }
        
        .sidebar.closed .w-full > svg,
        .sidebar.closed .w-full .font-sans,
        .sidebar.closed .text-blg {
            opacity: 0;
            display: none;
            transition: opacity 0.3s ease;
        }

        .sidebar.closed .closeButton {
            rotate: 180deg;
        }

        .sidebar.closed .closeButtonContainer {
            justify-content: center;
        }
        
        .sidebar .w-full > div,
        .sidebar .w-full > svg,
        .sidebar .text-blg {
            opacity: 1;
        }
    }
        </style>
       
</head>
